---
import NavBar from "../components/NavBar.astro";
import TextOnHeader from "../components/TextOnHeader.astro";
import ContactItems from "../components/ContactItems.astro";

const { subTitle, Title } = Astro.props;
---
<header class="inner-hero">
    <NavBar />
    <div class="container">
        <div class="row">
            <div class="col-12 col-xl-3">
                <TextOnHeader subTitle={subTitle} Title={Title} />
            </div>
        </div>
    </div>
    
    <ContactItems />
</header>
